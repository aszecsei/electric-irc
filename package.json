{
  "name": "electric-irc",
  "version": "0.0.1",
  "contributors": [
    "Alic Szecsei <alic-szecsei@uiowa.edu>",
    "Tanner Brunscheon <tanner-brunscheon@uiowa.edu>",
    "Willem DeJong <willem-dejong@uiowa.edu>",
    "Vincent Lopez <vincent-lopez@uiowa.edu>"
  ],
  "repository": "github:aszecsei/electric-irc",
  "description": "A modern, stylish IRC client using Electron and React",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "start": "yarn run dev",
    "test": "cross-env TS_NODE_PROJECT='test/' nyc mocha",
    "release": "yarn compile && electron-builder -mwl --ia32 --x64",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote \"./**/*.{ts,tsx,scss}\""
  },
  "lint-staged": {
    "{app,test}/**/*.{ts,tsx,scss}": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "dist",
      "app/main",
      "coverage"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "lines": 20,
    "statements": 20,
    "functions": 0,
    "branches": 0,
    "check-coverage": true,
    "all": true
  },
  "main": "app/main/index.ts",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/chai-enzyme": "^0.6.3",
    "@types/classnames": "^2.2.3",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/irc": "^0.3.32",
    "@types/mocha": "^2.2.46",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-fontawesome": "^1.6.1",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "@types/webdriverio": "^4.8.7",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "electron": "^1.7.10",
    "electron-builder": "^20.0.4",
    "electron-devtools-installer": "^2.2.3",
    "electron-mocha": "^5.0.0",
    "electron-webpack": "^1.11.0",
    "electron-webpack-ts": "^1.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.1",
    "sinon-chai": "^2.14.0",
    "spectron": "^3.7.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@types/reactstrap": "^5.0.16",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "electron-store": "^1.3.0",
    "irc": "^0.5.2",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "popper.js": "^1.12.9",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "reactstrap": "^5.0.0-beta",
    "source-map-support": "^0.5.1",
    "typeface-roboto": "^0.0.54"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
