{
  "name": "electric-irc",
  "version": "0.0.1",
  "contributors": [
    "Alic Szecsei <alic-szecsei@uiowa.edu>",
    "Tanner Brunscheon <tanner-brunscheon@uiowa.edu>",
    "Willem DeJong <willem-dejong@uiowa.edu>",
    "Kyle Anderson <kyle-l-anderson@uiowa.edu>",
    "Vincent Lopez <vincent-lopez@uiowa.edu>"
  ],
  "description": "A modern, stylish IRC client using Electron and React",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "start": "yarn run dev",
    "test": "cross-env TS_NODE_PROJECT='test/' nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "dist",
      "coverage"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/chai-enzyme": "^0.6.3",
    "@types/classnames": "^2.2.3",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/mocha": "^2.2.46",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-fontawesome": "^1.6.1",
    "@types/webdriverio": "^4.8.7",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.3",
    "electron": "^1.7.10",
    "electron-builder": "^19.54.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-webpack": "^1.11.0",
    "electron-webpack-ts": "^1.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.6",
    "spectron": "^3.7.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-store": "^1.3.0",
    "material-design-icons": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "source-map-support": "^0.5.1",
    "typeface-roboto": "^0.0.50"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
