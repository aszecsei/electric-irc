{
    "include": [
        "app/renderer/**/*.ts",
        "app/renderer/**/*.tsx",
        "app/common/**/*.ts",
        "app/common/**/*.tsx"
    ],
    "extension": [
        ".ts",
        ".tsx"
    ],
    "exclude": [
        "**/*.d.ts",
        "test",
        "dist",
        "app/main",
        "coverage"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "lcov",
        "text",
        "html"
    ],
    "lines": 20,
    "statements": 20,
    "functions": 0,
    "branches": 0,
    "check-coverage": true,
    "all": true,
    "sourceMap": true,
    "instrument": true
}